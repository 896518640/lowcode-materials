<% if (includeContext) { %>
import useContext from './useContext';
<% } %>
<% if (includeService) { %>
import useService from './useService';
<% } %>
<% if (!includeService) { %>
import useModel from './useModel';
<% } %>

const useController = () => {
  <% if (includeContext) { %>
  const context = useContext();
  <% } %>
  <% if (includeService) { %>
  const service = useService();
  const { model } = service ;
  <% } %>
  <% if (!includeService) { %>
  const model = useModel();
  <% } %>

  return {
	<% if (includeService) { %>
    service,
	<% } %>
	<% if (!includeService) { %>
	model,
	<% } %>
  };
};

export default useController;
