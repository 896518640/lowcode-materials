<!-- lowcode-vue-template -->
<a-descriptions 
	title="<%= title %>" 
	:bordered="<%= bordered %>"
	<% if(column){ _%>
	:column="<%= column %>"
	<% } _%>
	<% if(layout){ _%>
	:column="<%= layout %>"
	<% } _%>
>
	<% if(extra){ _%>
	<template #extra>
		extra
	</template>
	<% } _%>
	<% items.map((item, index) => { _%>
		<a-descriptions-item 
			label="<%= item.label %>"
			<% if(item.span){ _%>
				:span="<%= item.span %>"
			<% } _%>
		>
			{{ model.detailInfo.value?.<%= item.key || `item${index+1}` %> }}
		</a-descriptions-item>
	<% }) _%>
</a-descriptions>

